								<DataLayer
									ConnectionID="OpenDSP"
									ID="dlCampMonthly"
									Source="SELECT DATE(cm.report_date) report_date,
MONTHNAME(cm.report_date) report_month,
c.`name` Campaign,
cm.campaign_id,
cm.user_count
FROM stat.impression_unique_user_hll_campaign_monthly cm
