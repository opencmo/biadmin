										DataColumn="report_datetime"
										DataType="Date"
									/>
								</DataTableColumn>
								<DataTableColumn
									Header="Campaign"
									ID="colCampaign"
									>
									<Label
										Caption="@Data.Campaign~"
										ID="lblCampaign"
									/>
									<DataColumnSort
										DataColumn="Campaign"
									/>
