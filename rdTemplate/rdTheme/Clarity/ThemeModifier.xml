<TemplateModifier>
	<!-- StyleSheet -->
	<PrependChildElement XPath="/MobileReport" >
		<NewElement>
			<StyleSheet StyleSheet="rdTemplate/rdTheme/Clarity/MobileTheme.css" />
		</NewElement>
    </PrependChildElement>

	<PrependChildElement XPath="/Report | /MobileReport | /Widget" >
		<NewElement>
			<StyleSheet StyleSheet="rdTemplate/rdRoundCorners.css" />
		</NewElement>
    </PrependChildElement>

    <PrependChildElement XPath="/Report | /MobileReport | /Widget" >
		<NewElement>
			<StyleSheet StyleSheet="rdTemplate/rdTheme/Clarity/Theme.css" />
		</NewElement>
    </PrependChildElement>

	<!-- WaitPage -->
	<SetAttributeWhenEmpty XPath=".//WaitPage" Class="rdThemeWaitPanel" CaptionClass="rdThemeWaitCaption" StyleSheet="rdTemplate/rdTheme/Clarity/Theme.css" />

	<!-- Tabs -->
	<SetAttributeWhenEmpty XPath=".//Tabs" BorderColor="#c1c1c1" ActiveTabBackgroundColor="#ececec" InactiveTabBackgroundColor="#898989" />
	<SetAttribute XPath="//Tabs[@ID='rdDashboardTabs']" ActiveTabBackgroundColor="#ececec" />

	<!-- DataTable -->
	<SetAttributeWhenEmpty XPath=".//DataTable" CellSpacing="1" />
	<SetAttributeWithInsert XPath=".//DataTable[not(ancestor::Report[@ID='AnalysisGrid.rdAgTemplate']) and not(@rdAnalysisGrid='True')]" Class="rdThemeDataTable" ColumnHeaderClass="rdThemeDataTableHeader" CaptionClass="rdThemeDataTableMainHeader" />
	<SetAttributeWithInsert XPath=".//DataTable/DataTableColumn | DataTableColumn" Class="rdThemeDataTableCell" />
	<SetAttributeWithInsert XPath=".//DataTable/HeaderRow" Class="rdThemeDataTableHeaderRow" />
	<SetAttributeWithInsert XPath=".//DataTable/SummaryRow" Class="rdThemeDataTableSummaryRow" />
	<CycleAttributeValues XPath=".//DataTable" CycleOnChildElement="GroupHeaderRow">
		<CycleValue Class="rdThemeDataTableGroupHeaderRow0" />
		<CycleValue Class="rdThemeDataTableGroupHeaderRow1" />
		<CycleValue Class="rdThemeDataTableGroupHeaderRow2" />
		<CycleValue Class="rdThemeDataTableGroupHeaderRow3" />
		<CycleValue Class="rdThemeDataTableGroupHeaderRow4" />
	</CycleAttributeValues>
	<SetAttributeWithInsert XPath=".//DataTable/GroupSummaryRow" Class="rdThemeDataTableGroupSummaryRow" />
	<SetAttributeWithInsert XPath=".//DataTable/MoreInfoRow" Class="rdThemeDataTableMoreInfoRow" />

	<!-- SubDataTable -->
	<SetAttributeWhenEmpty XPath=".//SubDataTable" CellSpacing="1" />
	<SetAttributeWithInsert XPath=".//SubDataTable" Class="rdThemeSubDataTable" />
	<SetAttributeWithInsert XPath=".//SubDataTable/DataTableColumn" Class="rdThemeSubDataTableDataTableCell" />
	<SetAttributeWithInsert XPath=".//SubDataTable/HeaderRow" Class="rdThemeSubDataTableHeaderRow" />
	<SetAttributeWithInsert XPath=".//SubDataTable/SummaryRow" Class="rdThemeSubDataTableSummaryRow" />
	<SetAttributeWithInsert XPath=".//SubDataTable/GroupHeaderRow" Class="rdThemeSubDataTableGroupHeaderRow" />
	<SetAttributeWithInsert XPath=".//SubDataTable/GroupSummaryRow" Class="rdThemeSubDataTableGroupSummaryRow" />
	<SetAttributeWithInsert XPath=".//SubDataTable/MoreInfoRow" Class="rdThemeSubDataTableMoreInfoRow" />

	<!-- Paging -->
	<SetAttributeWithInsert XPath=".//InteractivePaging" Class="rdThemePaging" CurrentPageClass="rdThemePagingCurrent" />
	<SetAttribute XPath=".//AnalysisGrid" CurrentPageClass="rdThemePagingCurrent" />
	<SetAttribute XPath=".//InteractivePaging[@CaptionType='Image']" FirstPageCaption="rdTemplate/rdTheme/Silver/rdPageFirst.gif" PreviousPageCaption="rdTemplate/rdTheme/Silver/rdPagePrevious.gif" NextPageCaption="rdTemplate/rdTheme/Silver/rdPageNext.gif" LastPageCaption="rdTemplate/rdTheme/Silver/rdPageLast.gif" />
