	<UpdateOrAppendChildElement XPath=".//Chart[@Type='XY']">
		<NewElement>
			<FontLabel FontFilename="Arial" FontColor="#464646" BorderColor="#c0c0c0" FontSize="11" />
		</NewElement>
	</UpdateOrAppendChildElement>
	<UpdateOrAppendChildElement XPath=".//Chart[@Type='XY']">
		<NewElement>
			<FontData  FontFilename="Arial" FontColor="#464646" FontSize="11" />
		</NewElement>
	</UpdateOrAppendChildElement>
	<UpdateOrAppendChildElement XPath=".//Chart[@Type='XY']">
		<NewElement>
			<FontXAxisTitle FontFilename="Arial" FontColor="#464646" FontSize="11" />
