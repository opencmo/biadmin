			<FontYAxisTitle FontFilename="Arial" FontColor="#464646" FontSize="11" />
		</NewElement>
	</UpdateOrAppendChildElement>
	<CycleAttributeValues XPath=".//Chart[@Type='XY']" CycleOnChildElement="ExtraXYDataColumn">
		<CycleValue Transparency="3" Color="#ff9900" />
		<CycleValue Transparency="3" Color="#ffb444" />
		<CycleValue Transparency="3" Color="#ffd088" />
		<CycleValue Transparency="3" Color="#ffebcc" />
	</CycleAttributeValues>
	<CycleAttributeValues XPath=".//Chart[@Type='XY']" CycleOnChildElement="ExtraXYLayer">
		<CycleValue ChartSymbolEdgeColor="Transparent" Transparency="3" Color="#ff9900" />
		<CycleValue ChartSymbolEdgeColor="Transparent" Transparency="3" Color="#ffb444" />
		<CycleValue ChartSymbolEdgeColor="Transparent" Transparency="3" Color="#ffd088" />
		<CycleValue ChartSymbolEdgeColor="Transparent" Transparency="3" Color="#ffebcc" />
	</CycleAttributeValues>
	<CycleAttributeValues XPath=".//Chart[@Type='XY']" CycleOnChildElement="ChartMarks">
		<CycleValue Transparency="3" Color="#ff9900" />
		<CycleValue Transparency="3" Color="#ffb444" />
		<CycleValue Transparency="3" Color="#ffd088" />
		<CycleValue Transparency="3" Color="#ffebcc" />
	</CycleAttributeValues>
	<CycleAttributeValues XPath=".//Chart[@Type='XY']/SecondaryAxis" CycleOnChildElement="ChartMarks">
		<CycleValue Color="#ff9900" />
		<CycleValue Color="#ffb444" />
		<CycleValue Color="#ffd088" />
		<CycleValue Color="#ffebcc" />
	</CycleAttributeValues>

	<!-- ZoomChart -->
	<SetAttributeWhenEmpty XPath=".//ZoomChart" ResetZoomClass="rdDashboardCommand" />

	<!-- PieChart -->
<!-- No longer using a single wedge color for internal labels
	<SetAttributeWhenEmpty XPath=".//Chart[@Type='Pie' and @LabelLayout='InternalLayout']" Colors="#75a4fb" />-->
	<SetAttributeWhenEmpty XPath=".//Chart[@Type='Pie']" LabelRoundedBorder="True" RoundedBorder="4" WedgeBorderThickness="1" BackgroundColor="Transparent" ChartSymbolEdgeColor="Transparent" OuterBorderColor="Transparent" D3Angle="" D3="" ShowDataValues="False" LabelPointerColor="SameAsWedgeColor" Colors="#ff9900, #ffb444, #7fc659, #b4d7a1, #75a4fb, #c6d9fd, #e23d26, #fa7a67, #f7c906, #fdea6b" />
	<UpdateOrAppendChildElement XPath=".//Chart[@Type='Pie']">
		<NewElement>
			<FontChartTitle FontFilename="Arial bold" FontColor="#464646" FontSize="18" Alignment="Left" />
		</NewElement>
	</UpdateOrAppendChildElement>
	<UpdateOrAppendChildElement XPath=".//Chart[@Type='Pie' and @LabelLayout='InternalLayout']">
		<NewElement>
			<FontLabel FontColor="#ffffff" />
		</NewElement>
