<TemplateModifier>
	<!-- StyleSheet -->
	<PrependChildElement XPath="/MobileReport" >
		<NewElement>
			<StyleSheet StyleSheet="rdTemplate/rdTheme/SimpleBlue/MobileTheme.css" />
		</NewElement>
    </PrependChildElement>

	<PrependChildElement XPath="/Report | /MobileReport | /Widget" >
		<NewElement>
			<StyleSheet StyleSheet="rdTemplate/rdRoundCorners.css" />
		</NewElement>
    </PrependChildElement>

    <PrependChildElement XPath="/Report | /MobileReport | /Widget" >
		<NewElement>
			<StyleSheet StyleSheet="rdTemplate/rdTheme/SimpleBlue/Theme.css" />
		</NewElement>
    </PrependChildElement>

	<!-- WaitPage -->
	<SetAttributeWhenEmpty XPath=".//WaitPage" Class="rdThemeWaitPanel" CaptionClass="rdThemeWaitCaption" StyleSheet="rdTemplate/rdTheme/SimpleBlue/Theme.css" />

	<!-- Tabs -->
	<SetAttributeWhenEmpty XPath=".//Tabs" BorderColor="#c1c1c1" ActiveTabBackgroundColor="#e8eef7" InactiveTabBackgroundColor="#6b90da" />
	<SetAttribute XPath="//Tabs[@ID='rdDashboardTabs']" BorderColor="#c1c1c1" ActiveTabBackgroundColor="#e8eef7" />

	<!-- DataTable -->
	<SetAttributeWhenEmpty XPath=".//DataTable" CellSpacing="1" />
	<SetAttributeWithInsert XPath=".//DataTable[not(ancestor::Report[@ID='AnalysisGrid.rdAgTemplate']) and not(@rdAnalysisGrid='True')]" Class="rdThemeDataTable" ColumnHeaderClass="rdThemeDataTableHeader" CaptionClass="rdThemeDataTableMainHeader" />
	<SetAttributeWithInsert XPath=".//DataTable/DataTableColumn | DataTableColumn" Class="rdThemeDataTableCell" />
	<SetAttributeWithInsert XPath=".//DataTable/HeaderRow" Class="rdThemeDataTableHeaderRow" />
	<SetAttributeWithInsert XPath=".//DataTable/SummaryRow" Class="rdThemeDataTableSummaryRow" />
	<CycleAttributeValues XPath=".//DataTable" CycleOnChildElement="GroupHeaderRow">
		<CycleValue Class="rdThemeDataTableGroupHeaderRow0" />
		<CycleValue Class="rdThemeDataTableGroupHeaderRow1" />
		<CycleValue Class="rdThemeDataTableGroupHeaderRow2" />
		<CycleValue Class="rdThemeDataTableGroupHeaderRow3" />
		<CycleValue Class="rdThemeDataTableGroupHeaderRow4" />
	</CycleAttributeValues>
	<SetAttributeWithInsert XPath=".//DataTable/GroupSummaryRow" Class="rdThemeDataTableGroupSummaryRow" />
	<SetAttributeWithInsert XPath=".//DataTable/MoreInfoRow" Class="rdThemeDataTableMoreInfoRow" />

	<!-- SubDataTable -->
	<SetAttributeWhenEmpty XPath=".//SubDataTable" CellSpacing="1" />
	<SetAttributeWithInsert XPath=".//SubDataTable" Class="rdThemeSubDataTable" />
	<SetAttributeWithInsert XPath=".//SubDataTable/DataTableColumn" Class="rdThemeSubDataTableDataTableCell" />
	<SetAttributeWithInsert XPath=".//SubDataTable/HeaderRow" Class="rdThemeSubDataTableHeaderRow" />
	<SetAttributeWithInsert XPath=".//SubDataTable/SummaryRow" Class="rdThemeSubDataTableSummaryRow" />
	<SetAttributeWithInsert XPath=".//SubDataTable/GroupHeaderRow" Class="rdThemeSubDataTableGroupHeaderRow" />
	<SetAttributeWithInsert XPath=".//SubDataTable/GroupSummaryRow" Class="rdThemeSubDataTableGroupSummaryRow" />
	<SetAttributeWithInsert XPath=".//SubDataTable/MoreInfoRow" Class="rdThemeSubDataTableMoreInfoRow" />

	<!-- Paging -->
	<SetAttributeWithInsert XPath=".//InteractivePaging" Class="rdThemePaging" CurrentPageClass="rdThemePagingCurrent" />
	<SetAttribute XPath=".//AnalysisGrid" Class="rdThemePaging" CurrentPageClass="rdThemePagingCurrent" />
	<SetAttribute XPath=".//InteractivePaging[@CaptionType='Image']" FirstPageCaption="rdTemplate/rdTheme/SimpleBlue/rdPageFirst.gif" PreviousPageCaption="rdTemplate/rdTheme/SimpleBlue/rdPagePrevious.gif" NextPageCaption="rdTemplate/rdTheme/SimpleBlue/rdPageNext.gif" LastPageCaption="rdTemplate/rdTheme/SimpleBlue/rdPageLast.gif" />
	<SetAttribute XPath=".//InteractivePaging[@CaptionType='Image' and @FirstPageCaption='page_first_en.gif' ]" FirstPageCaption="rdTemplate/rdTheme/SimpleBlue/rdPageFirst.gif" />
	<SetAttribute XPath=".//InteractivePaging[@CaptionType='Image' and @PreviousPageCaption='page_prev_en.gif']" PreviousPageCaption="rdTemplate/rdTheme/SimpleBlue/rdPagePrevious.gif" />
	<SetAttribute XPath=".//InteractivePaging[@CaptionType='Image' and @NextPageCaption='page_next_en.gif']" NextPageCaption="rdTemplate/rdTheme/SimpleBlue/rdPageNext.gif" />
	<SetAttribute XPath=".//InteractivePaging[@CaptionType='Image' and @LastPageCaption='page_last_en.gif']" LastPageCaption="rdTemplate/rdTheme/SimpleBlue/rdPageLast.gif" />
	<SetAttributeWithInsert XPath=".//AppendPaging" Class="ThemeLinkButton" />

	<!-- CrossTabTable -->
	<SetAttributeWhenEmpty XPath=".//CrosstabTable" CellSpacing="1" />
	<SetAttributeWithInsert XPath=".//CrosstabTable[not(ancestor::Report[@ID='AnalysisGrid.rdAgTemplate']) and not(@rdAnalysisGrid='True')]" Class="rdThemeDataTable" ColumnHeaderClass="rdThemeDataTableHeader" />
	<SetAttributeWithInsert XPath=".//CrosstabTableLabelColumn" Class="rdThemeDataTableCell" />
	<SetAttributeWithInsert XPath=".//CrosstabTableValueColumns" Class="rdThemeDataTableCell ThemeAlignRight" />
	<SetAttributeWithInsert XPath=".//CrosstabTable/HeaderRow" Class="rdThemeDataTableHeaderRow" />
	<SetAttributeWithInsert XPath=".//CrosstabTable/SummaryRow" Class="rdThemeDataTableSummaryRow" />

	<!-- Analysis, OLAP Grid -->
	<SetAttributeWithInsert XPath=".//AnalysisGrid/AnalysisGridColumn | AnalysisGridColumn" Class="rdThemeDataTableCell" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate']//DataTable[@ID='dtLayout' or @ID='dtGroups' or @ID='dtOrders' or @ID='dtAggrs' or @ID='dtCharts' or @ID='dtCrosstabs' or @ID='dtCalcColumns' or @ID='dtFilters']" Class="rdRemoveTableStyle" />
	<SetAttribute XPath="/Report[@ID='OlapGrid.rdOgTemplate']//DataTableColumn[@ID='colPickedFilters'] | /Report[@ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='ChartGrid.rdCgTemplate']//DataTable[@ID='dtPickedFilters' or @ID='dtCalcMeasures' or @ID='mclDimensions' or @ID='mclMeasures' or @ID='mclFilters' or @ID='dtGridYAxis' or @ID='dtGridXAxis' or @ID='dtGridColumns' or @ID='dtGridRows' or @ID='dtGridFilter'] | /Report[@ID='OlapGrid.rdOgTemplate']//OlapTable[@ID='otFilterTable']" Class="rdRemoveTableStyle" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='TrellisChart.rdTcTemplate' or @ID='AnalysisChart.rdAcTemplate']//Chart" BackgroundColor="#ffffff" Transparency="" D3Angle="" D3="" ChartTexture="" LabelShading="" OuterBorderColor="#cccccc" BorderColor="Transparent" GridHorizontalColor="#f0eaea" ChartBackgroundColor="#ffffff" LineWidth="3" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='TrellisChart.rdTcTemplate' or @ID='AnalysisChart.rdAcTemplate']//Chart/FontChartTitle" FontFilename="Arial bold" FontColor="#464646" FontSize="18" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='TrellisChart.rdTcTemplate' or @ID='AnalysisChart.rdAcTemplate']//Chart/FontLabel" FontFilename="Arial" FontColor="#464646" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='TrellisChart.rdTcTemplate' or @ID='AnalysisChart.rdAcTemplate']//Chart/HeatmapFontTitle" FontFilename="Arial" FontBold="True" FontColor="#464646" FontSize="18" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='TrellisChart.rdTcTemplate' or @ID='AnalysisChart.rdAcTemplate']//Chart[@Type='XY' or @Type='Scatter']"  Transparency="5" BarShading="" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='TrellisChart.rdTcTemplate' or @ID='AnalysisChart.rdAcTemplate']//Chart[@Type='Scatter']" BorderColor="#c0c0c0" ChartSymbolEdgeColor="#0b6bcf" ChartSymbolFillColor="#0b6bcf" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='AnalysisChart.rdAcTemplate']//Chart[@Type='Pie']" LabelRoundedBorder="True" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='TrellisChart.rdTcTemplate' or @ID='AnalysisChart.rdAcTemplate']//ChartTrendLine" LineWidth="3" Color="#f7c906" />
	<SetAttribute XPath="/Report[@ID='TrellisChart.rdTcTemplate']//Chart[@Type='XY']" LineWidth="2" D3="0" />
	<SetAttribute XPath="/Report[@ID='TrellisChart.rdTcTemplate']//Chart[@Type='Scatter']" ChartSymbolSize="5" ChartSymbol="SymbolCircle" />
	<UpdateOrAppendChildElement XPath="/Report[@ID='OlapGrid.rdDgTemplate' or @ID='TrellisChart.rdTcTemplate' or @ID='OlapGrid.rdOgTemplate']//Chart[@Type='XY']">
		<NewElement>
			<DimensionGridChartPanel Colors="#4d89f9,#ff9900,#ffb444,#ffd088,#ffebcc" />
		</NewElement>
	</UpdateOrAppendChildElement>
	<UpdateOrAppendChildElement XPath="/Report[@ID='TrellisChart.rdTcTemplate']//Chart[@Type='XY' or @Type='Scatter']">
		<NewElement>
			<FontLabel FontSize="8" />
		</NewElement>
	</UpdateOrAppendChildElement>
	<UpdateOrAppendChildElement XPath="/Report[@ID='TrellisChart.rdTcTemplate']//Chart[@Type='XY' or @Type='Scatter']">
		<NewElement>
			<FontData FontSize="8" />
		</NewElement>
	</UpdateOrAppendChildElement>

	<!-- OlapTable-->
	<SetAttributeWhenEmpty XPath=".//OlapTable" CellSpacing="1" />
	<SetAttributeWithInsert XPath=".//OlapTable" Class="rdThemeDataTable" ColumnHeaderClass="rdThemeDataTableHeader" CaptionClass="rdThemeDataTableMainHeader" />
	<SetAttributeWithInsert XPath=".//OlapTable/DataTableColumn | DataTableColumn" Class="rdThemeDataTableCell" />

	<!-- PopupMenu -->
	<SetAttributeWithInsert XPath=".//Action[@Type='Popup']" Class="rdThemePopupMenu" />

	<!-- Charts -->

	<SetAttributeWhenEmpty XPath=".//ChartTrendLine" LineWidth="3" Color="#f7c906" />
	<SetAttributeWhenEmpty XPath=".//ChartLowessCurveLine" LineWidth="3" Color="#f7c906" />

	<!-- Legend -->
	<SetAttributeWhenEmpty XPath=".//Legend" BorderColor="Transparent" />
	<UpdateOrAppendChildElement XPath=".//Legend">
		<NewElement>
			<FontLegend FontFilename="Arial" FontColor="#464646" FontSize="11" />
		</NewElement>
	</UpdateOrAppendChildElement>

	<!-- XYChart -->
	<SetAttributeWhenEmpty XPath=".//Chart[@Type='XY' and DataLayer/CrosstabFilter]" Color="#ff9900, #ffb444, #7fc659, #b4d7a1, #75a4fb, #c6d9fd, #e23d26, #fa7a67, #f7c906, #fdea6b" />
