	<!-- AnimatedPie -->
	<SetAttributeWhenEmpty XPath=".//AnimatedChart[@Type='Pie']" D3Effects="False" WedgeBorderColor="#ffffff" WedgeBorderThickness="1.5" Colors="#ff9900, #ffb444, #7fc659, #b4d7a1, #75a4fb, #c6d9fd, #e23d26, #fa7a67, #f7c906, #fdea6b" />
