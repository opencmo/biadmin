    <NewElement>
      <FontLabel FontFilename="Arial" FontColor="#464646" FontSize="15" Bold="0"/>
    </NewElement>
  </UpdateOrAppendChildElement>
  <UpdateOrAppendChildElement XPath=".//AnimatedChart[@Type='Pie']">
    <NewElement>
      <FontLegend FontFilename="Arial" FontColor="#464646" FontSize="15" Bold="0"/>
    </NewElement>
  </UpdateOrAppendChildElement>
  <UpdateOrAppendChildElement XPath=".//AnimatedChart[@Type='Pie']">
    <NewElement>
      <FontData FontFilename="Arial" FontColor="#464646" FontSize="15" Bold="0"/>
    </NewElement>
  </UpdateOrAppendChildElement>
  <UpdateOrAppendChildElement XPath=".//AnimatedChart[@Type='Pie']">
    <NewElement>
      <FontChartTitle FontFilename="Arial" FontColor="#464646" FontSize="24" Bold="1" />
    </NewElement>
  </UpdateOrAppendChildElement>
  <UpdateOrAppendChildElement XPath=".//AnimatedChart[@Type='Pie']">
    <NewElement>
      <FontTooltip FontFilename="Arial" FontColor="#464646" FontSize="15" Bold="0" />
    </NewElement>
  </UpdateOrAppendChildElement>

	<!-- AnimatedXY -->
  <SetAttributeWhenEmpty XPath=".//AnimatedChart[@Type='XY' and @XYChartType='Line']" LineWidth="3.5" />
  <SetAttributeWhenEmpty XPath=".//AnimatedChart[@Type='XY' and DataLayer/CrosstabFilter]" Color="#ff9900,#ffb444,#7fc659,#b4d7a1,#75a4fb,#c6d9fd,#e23d26,#fa7a67,#f7c906,#fdea6b" />
  <SetAttributeWhenEmpty XPath=".//AnimatedChart[@Type='XY']" BarStyle="Flat" Color="#4d89f9" D3="False" DivLineColor="#cccccc" DivLineThickness="2" ShowBorder="1" CanvasBgColor="#ffffff" AlternatingBackgroundColor="False" AnchorAlpha="0" AreaGradientColor="#4d89f9" />
  <SetAttributeWhenEmpty XPath=".//AnimatedChart[@XYChartType='Area' and not(ExtraXYDataColumn)]" Transparency="3" PlotFillAlpha="100" PlotBorderColor="#4d89f9"  />
  <!-- Hide canvas base and gradient for 3d xy charts-->
  <SetAttributeWhenEmpty XPath=".//AnimatedChart[@Type='XY' and @D3='True' and @XYChartType='Bar']" SlimCanvasBase="True"  D3Effects="False" />
  <UpdateOrAppendChildElement XPath=".//AnimatedChart[@Type='XY']">
    <NewElement>
      <FontLabel FontFilename="Arial" FontColor="#464646" FontSize="15" Bold="0"/>
    </NewElement>
  </UpdateOrAppendChildElement>
  <UpdateOrAppendChildElement XPath=".//AnimatedChart[@Type='XY']">
    <NewElement>
      <FontLegend FontFilename="Arial" FontColor="#464646" FontSize="15" Bold="0"/>
    </NewElement>
  </UpdateOrAppendChildElement>
  <UpdateOrAppendChildElement XPath=".//AnimatedChart[@Type='XY']">
    <NewElement>
      <FontData FontFilename="Arial" FontColor="#464646" FontSize="15" Bold="0"/>
    </NewElement>
  </UpdateOrAppendChildElement>
  <UpdateOrAppendChildElement XPath=".//AnimatedChart[@Type='XY']">
    <NewElement>
      <FontChartTitle FontFilename="Arial" FontColor="#464646" FontSize="24" Bold="1" />
    </NewElement>
  </UpdateOrAppendChildElement>
  <UpdateOrAppendChildElement XPath=".//AnimatedChart[@Type='XY']">
    <NewElement>
      <FontTooltip FontFilename="Arial" FontColor="#464646" FontSize="15" Bold="0" />
    </NewElement>
  </UpdateOrAppendChildElement>
	<CycleAttributeValues XPath=".//AnimatedChart[@Type='XY']" CycleOnChildElement="ExtraXYDataColumn">
		<CycleValue Color="#ff9900" AreaGradientColor="#ff9900" LineWidth="3.5"/>
		<CycleValue Color="#ffb444" AreaGradientColor="#ffb444" LineWidth="3.5"/>
		<CycleValue Color="#ffd088" AreaGradientColor="#ffd088" LineWidth="3.5"/>
		<CycleValue Color="#ffebcc" AreaGradientColor="#ffebcc" LineWidth="3.5"/>
	</CycleAttributeValues>

	<!-- AnimatedScatter -->
	<SetAttributeWhenEmpty XPath=".//AnimatedChart[@Type='Scatter']" ChartSymbolFillColor="#4d89f9" ChartBackgroundColor="#ffffff" AnchorRadius="3" Transparency="3" ChartSymbolSides="10" AlternatingBackgroundColor="False" ChartSymbolEdgeColor="#6b90da" BorderColor="#c0c0c0" CanvasBorderThickness="1" />
  <UpdateOrAppendChildElement XPath=".//AnimatedChart[@Type='Scatter']">
    <NewElement>
      <FontLabel FontFilename="Arial" FontColor="#464646" FontSize="15" Bold="0"/>
    </NewElement>
  </UpdateOrAppendChildElement>
  <UpdateOrAppendChildElement XPath=".//AnimatedChart[@Type='Scatter']">
    <NewElement>
      <FontLegend FontFilename="Arial" FontColor="#464646" FontSize="15" Bold="0"/>
    </NewElement>
  </UpdateOrAppendChildElement>
  <UpdateOrAppendChildElement XPath=".//AnimatedChart[@Type='Scatter']">
    <NewElement>
      <FontData FontFilename="Arial" FontColor="#464646" FontSize="15" Bold="0"/>
    </NewElement>
  </UpdateOrAppendChildElement>
  <UpdateOrAppendChildElement XPath=".//AnimatedChart[@Type='Scatter']">
    <NewElement>
      <FontChartTitle FontFilename="Arial" FontColor="#464646" FontSize="24" Bold="1" />
    </NewElement>
  </UpdateOrAppendChildElement>
  <UpdateOrAppendChildElement XPath=".//AnimatedChart[@Type='Scatter']">
    <NewElement>
      <FontTooltip FontFilename="Arial" FontColor="#464646" FontSize="15" Bold="0" />
    </NewElement>
  </UpdateOrAppendChildElement>
	<CycleAttributeValues XPath=".//AnimatedChart[@Type='Scatter']" CycleOnChildElement="AnimatedExtraScatterLayer">
		<CycleValue ChartSymbolFillColor="#ff9900" ChartSymbolEdgeColor="#ffb444" ChartSymbolSize="3" Transparency="3" ChartSymbolSides="10"/>
		<CycleValue ChartSymbolFillColor="#ffd088" ChartSymbolEdgeColor="#ff9900" ChartSymbolSize="3" Transparency="3" ChartSymbolSides="10"/>
    <CycleValue ChartSymbolFillColor="#ffebcc" ChartSymbolEdgeColor="#ffb444" ChartSymbolSize="3" Transparency="3" ChartSymbolSides="10"/>
	</CycleAttributeValues>

	<!-- ChartCanvas -->

	<SetAttributeWhenEmpty XPath=".//ChartCanvas" PlotBackgroundColor="Transparent" BackgroundColor="Transparent" BorderRadius="4" BorderColor="Transparent" />
	<SetAttributeWhenEmpty XPath=".//ChartCanvas/ChartXAxis/ChartTicksAndGrid" GridLineColor="#f0eaea" GridLineThickness="3" />

	<!-- ChartCanvas series -->
	<SetAttributeWhenEmpty XPath=".//ChartCanvas[DataLayer/CrosstabFilter]/Series | .//ChartCanvas/Series[DataLayer/CrosstabFilter]" Color="#4d89f9, #ffb444, #7fc659, #97bbfc, #fa7a67, #f7e306" />
	<SetAttributeWhenEmpty XPath=".//ChartCanvas/Series" ChartSymbolEdgeColor="Transparent" Transparency="3" />
	<SetAttributeWhenEmpty XPath=".//ChartCanvas/Series[@BarStacking='Overlay']" Transparency="6" />

	<CycleAttributeValues XPath=".//ChartCanvas" CycleOnChildElement="Series">
		<CycleValue Color="#4d89f9" />
		<CycleValue Color="#ffb444" />
		<CycleValue Color="#7fc659" />
		<CycleValue Color="#97bbfc" />
		<CycleValue Color="#fa7a67" />
		<CycleValue Color="#f7e306" />
	</CycleAttributeValues>

	<!-- Legend -->
	<UpdateOrAppendChildElement XPath=".//ChartCanvas[DataLayer/CrosstabFilter] | .//ChartCanvas[Series/DataLayer/CrosstabFilter] | .//ChartCanvas[Series[@LegendLabel]]">
		<NewElement>
			<ChartCanvasLegend />
		</NewElement>
	</UpdateOrAppendChildElement>
	<SetAttributeWhenEmpty XPath=".//ChartCanvasLegend" BorderColor="Transparent" />
	<UpdateOrAppendChildElement XPath=".//ChartCanvasLegend[@LegendCaption]">
		<NewElement>
			<LegendCaptionStyle />
		</NewElement>
	</UpdateOrAppendChildElement>
	<SetAttributeWhenEmpty XPath=".//ChartCanvasLegend[@LegendCaption]/LegendCaptionStyle" FontFamily="Helvetica,Arial,sans-serif" FontWeight="Normal" FontColor="#464646" FontSize="18" />
	<UpdateOrAppendChildElement XPath=".//ChartCanvasLegend">
		<NewElement>
			<LegendLabelStyle />
		</NewElement>
	</UpdateOrAppendChildElement>
	<SetAttributeWhenEmpty XPath=".//ChartCanvasLegend/LegendLabelStyle" FontFamily="Helvetica,Arial,sans-serif" FontColor="#464646" FontSize="14" />
	<UpdateOrAppendChildElement XPath=".//ChartCanvasLegend">
		<NewElement>
			<LegendNavigationStyle />
		</NewElement>
	</UpdateOrAppendChildElement>
	<SetAttributeWhenEmpty XPath=".//ChartCanvasLegend/LegendNavigationStyle" FontFamily="Helvetica,Arial,sans-serif" FontColor="#464646" FontSize="14" />

	<!-- ChartCanvas Fonts -->
		<!-- chart title -->
	<UpdateOrAppendChildElement XPath=".//ChartCanvas[@ChartCaption]">
		<NewElement>
			<ChartCaptionStyle />
		</NewElement>
	</UpdateOrAppendChildElement>
	<SetAttributeWhenEmpty XPath=".//ChartCanvas/ChartCaptionStyle" FontFamily="Helvetica,Arial,sans-serif" FontWeight="Bold" FontColor="#464646" FontSize="24" AlignmentHorizontal="Center" /> <!-- OffsetY="32" -->
		<!-- chart subtitle -->
	<UpdateOrAppendChildElement XPath=".//ChartCanvas[@ChartSubCaption]">
		<NewElement>
			<ChartSubCaptionStyle />
		</NewElement>
	</UpdateOrAppendChildElement>
	<SetAttributeWhenEmpty XPath=".//ChartCanvas/ChartSubCaptionStyle" FontFamily="Helvetica,Arial,sans-serif" FontWeight="Normal" FontColor="#464646" FontSize="18" AlignmentHorizontal="Center" /> <!-- OffsetY="32" -->
		<!-- x- or y-axis title -->
	<UpdateOrAppendChildElement XPath=".//ChartCanvas/ChartXAxis[@AxisCaption]">
		<NewElement>
			<AxisCaptionStyle />
		</NewElement>
	</UpdateOrAppendChildElement>
	<UpdateOrAppendChildElement XPath=".//ChartCanvas/ChartYAxis[@AxisCaption]">
		<NewElement>
			<AxisCaptionStyle />
		</NewElement>
	</UpdateOrAppendChildElement>
	<SetAttributeWhenEmpty XPath=".//AxisCaptionStyle" FontFamily="Helvetica,Arial,sans-serif" FontWeight="Normal" FontColor="#464646" FontSize="14" />
		<!-- x- and y-axis must exist to theme -->
	<UpdateOrAppendChildElement XPath=".//ChartCanvas">
		<NewElement>
			<ChartXAxis />
		</NewElement>
	</UpdateOrAppendChildElement>
	<UpdateOrAppendChildElement XPath=".//ChartCanvas">
		<NewElement>
			<ChartYAxis />
		</NewElement>
	</UpdateOrAppendChildElement>
		<!-- x- or y-axis label -->
	<UpdateOrAppendChildElement XPath=".//ChartCanvas/ChartXAxis">
		<NewElement>
			<AxisLabelStyle />
		</NewElement>
	</UpdateOrAppendChildElement>
	<UpdateOrAppendChildElement XPath=".//ChartCanvas/ChartYAxis">
		<NewElement>
			<AxisLabelStyle />
		</NewElement>
	</UpdateOrAppendChildElement>
	<SetAttributeWhenEmpty XPath=".//AxisLabelStyle" FontFamily="Helvetica,Arial,sans-serif" FontWeight="Normal" FontColor="#464646" FontSize="14" />
		<!-- data labels -->
	<SetAttributeWhenEmpty XPath=".//ChartCanvas/Series/DataLabels" FontFamily="Helvetica,Arial,sans-serif" FontWeight="Normal" FontColor="#464646" FontSize="14" />

		<!-- chart marks -->
	<CycleAttributeValues XPath=".//ChartCanvas/ChartXAxis | .//ChartCanvas/ChartYAxis" CycleOnChildElement="MarkerBand">
		<CycleValue Color="#4d89f9" />
		<CycleValue Color="#ffb444" />
		<CycleValue Color="#7fc659" />
		<CycleValue Color="#97bbfc" />
		<CycleValue Color="#fa7a67" />
		<CycleValue Color="#f7e306" />
	</CycleAttributeValues>
	<CycleAttributeValues XPath=".//ChartCanvas/ChartXAxis | .//ChartCanvas/ChartYAxis" CycleOnChildElement="MarkerLine">
		<CycleValue Color="#4d89f9" />
		<CycleValue Color="#ffb444" />
		<CycleValue Color="#7fc659" />
		<CycleValue Color="#97bbfc" />
		<CycleValue Color="#fa7a67" />
		<CycleValue Color="#f7e306" />
	</CycleAttributeValues>

	<!-- ChartCanvas: XY (Area, Bar, Line, Spline, Whiskers) -->
	<SetAttributeWhenEmpty XPath=".//ChartCanvas[(Series[@Type='Area'][1]) and (Series[2] or DataLayer/CrosstabFilter)]" ChartSymbolEdgeColor="#ffffff" Transparency="3" />
	<SetAttributeWhenEmpty XPath=".//ChartCanvas[(Series[@Type='Area']) and not(Series[2]) and not(DataLayer/CrosstabFilter)]" ChartSymbolEdgeColor="#4d89f9" AreaGradientColor="#ffffff"  />
	<SetAttributeWhenEmpty XPath=".//ChartCanvas/Series[@Type='Bar']" Transparency="3" />
	<SetAttributeWhenEmpty XPath=".//ChartCanvas/Series[@Type='Bar' and not(@BarBorderColor)]" BarBorderThickness="0" />
	<SetAttributeWhenEmpty XPath=".//ChartCanvas/Series[@Type='Line']" LineWidth="3" />
	<SetAttributeWhenEmpty XPath=".//ChartCanvas/Series[@Type='Whiskers']" Transparency="" StemColorTransparency="" WhiskerColorTransparency="" WhiskerLineThickness="2" WhiskerColor="#ffb444" StemLineThickness="2" StemColor="#ffb444" />

	<!-- ChartCanvas: PieChart, PyramidChart, FunnelChart -->
	<SetAttribute XPath=".//ChartCanvas/Series[@Type='Pie' or @Type='Pyramid' or @Type='Funnel']" Color="" />
	<SetAttributeWhenEmpty XPath=".//ChartCanvas/Series[@Type='Pie' or @Type='Pyramid' or @Type='Funnel']" Colors="#4d89f9, #ffb444, #7fc659, #97bbfc, #fa7a67, #f7e306" PieBorderThickness="0" />
	<UpdateOrAppendChildElement XPath=".//ChartCanvas/Series[@Type='Pie' or @Type='Pyramid' or @Type='Funnel']">
		<NewElement>
			<DataLabels />
		</NewElement>
	</UpdateOrAppendChildElement>
	<SetAttribute XPath=".//ChartCanvas/Series[@Type='Pie' or @Type='Pyramid' or @Type='Funnel']/DataLabels" FontFamily="Helvetica,Arial,sans-serif" FontWeight="Normal" FontColor="#464646" FontSize="14" />

	<!-- ChartCanvas: Scatter/Bubble -->
	<SetAttributeWhenEmpty XPath=".//ChartCanvas/Series[@Type='Scatter' or @Type='Bubble']" Transparency="3" />
	<SetAttributeWhenEmpty XPath=".//ChartCanvas/Series[@Type='Scatter' or @Type='Bubble']/MarkerPoints" MarkerPointSymbol="Circle" MarkerPointFillColorTransparency="3" MarkerPointRadius="6" MarkerPointBorderThickness="1" MarkerPointBorderColor="#ffffff" />
	<CycleAttributeValues XPath=".//ChartCanvas" CycleOnChildElement="Series[@Type='Scatter' or @Type='Bubble']/MarkerPoints">
		<CycleValue MarkerPointFillColor="#4d89f9" />
		<CycleValue MarkerPointFillColor="#ffb444" />
		<CycleValue MarkerPointFillColor="#7fc659" />
		<CycleValue MarkerPointFillColor="#97bbfc" />
		<CycleValue MarkerPointFillColor="#fa7a67" />
		<CycleValue MarkerPointFillColor="#f7e306" />
	</CycleAttributeValues>


	<!-- ChartCanvas: Heatmap -->
	<SetAttributeWhenEmpty XPath=".//ChartCanvas/Series[@Type='Heatmap']" BackgroundColor="#ffffff" OuterBorderColor="Transparent" CellBorder="2" CellBorderColor="#ffffff" MediumValueColor="#FDEA6B" LowValueColor="#FF9900" HighValueColor="#7FC659" />


	<!-- Gauges -->

	<!-- NeedleGauge -->
	<SetAttributeWhenEmpty XPath=".//Gauge[@Type='Needle']" TickLabelFontName="Arial" D3Effects="True"  NeedleGaugeEnclosure="RoundGlass" BorderColor="DimGray" BackgroundColor="#c6d9fd" NeedleColor="#ff9900" TickColor="#ff9900" TickThickness="3" NeedleThickness="2" />
	<SetAttributeWhenEmpty XPath=".//GaugeRing" Color="#ffd088" RingThickness="2" />
	<SetAttributeWhenEmpty XPath=".//GaugeLabel" FontName="Arial"  FontColor="#464646" />
	<CycleAttributeValues XPath=".//Gauge[@Type='Needle']" CycleOnChildElement="ExtraNeedle">
		<CycleValue NeedleColor="#7fc659" />
		<CycleValue NeedleColor="#75a4fb" />
		<CycleValue NeedleColor="#e23d26" />
		<CycleValue NeedleColor="#f7c906" />
	</CycleAttributeValues>
	<!-- BarGauge -->
	<SetAttributeWhenEmpty XPath=".//Gauge[@Type='Bar']" FontName="Arial" D3Effects="True" BackgroundColor="#ffffff" BarColor="#4d89f9" />
	<!-- IndicatorGauge -->
	<SetAttributeWhenEmpty XPath=".//Gauge[@Type='Indicator']" BackgroundColor="#ffffff" />
	<SetAttributeWhenEmpty XPath=".//IndicatorState" BorderColor="#ff9900" Color="#ffb444" />
	<!-- AnimatedNeedle -->
	<SetAttributeWhenEmpty XPath=".//AnimatedGauge[@Type='Needle']" TickLabelFontName="Arial" OuterRingStyle="Lens" BackgroundColor="#e0ecff" />
	<!-- AnimatedBar -->
	<SetAttributeWhenEmpty XPath=".//AnimatedGauge[@Type='Bar']" TickLabelFontName="Arial" BackgroundColor="#e0ecff" BarColor="#4d89f9" />
	<CycleAttributeValues XPath=".//AnimatedGauge[@Type='Bar']" CycleOnChildElement="AnimatedGaugeLine">
		<CycleValue Color="#ff9900" />
		<CycleValue Color="#ffb444" />
		<CycleValue Color="#ffd088" />
		<CycleValue Color="#ffebcc" />
	</CycleAttributeValues>
	<!-- AnimatedPyramid -->
	<SetAttributeWhenEmpty XPath=".//AnimatedGauge[@Type='Pyramid']" BackgroundColor="#e0ecff" />

	<!-- DataCalendar -->
	<SetAttributeWithInsert XPath=".//DataCalendarRows//Label" Class="rdDataCalendarFont" />

	<!-- UserInput -->

	<!-- InputCheckBox -->
	<SetAttributeWithInsert XPath=".//InputCheckbox" CaptionClass="rdThemeInputCaption" />
	<!-- InputDate -->
	<SetAttributeWithInsert XPath=".//InputDate" Class="rdThemeInput" CaptionClass="rdThemeInputCaption" />
	<SetAttribute XPath=".//InputDate[not(@CalendarLink) and not(@CalendarLinkType)]" CalendarLinkType="Image" CalendarLinkClass="rdDataCalendarImage"  />
	<!-- InputTime -->
	<SetAttributeWithInsert XPath=".//InputTime" Class="rdThemeInput" CaptionClass="rdThemeInputCaption" />
	<SetAttribute XPath=".//InputTime[not(@ClockLink) and not(@ClockLinkType)]" ClockLinkType="Image" ClockLinkClass="rdTimePickerImage" />
