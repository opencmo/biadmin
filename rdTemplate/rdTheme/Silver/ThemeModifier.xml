		<NewElement>
			<FontChartTitle FontFilename="Arial bold" FontColor="#555555" FontSize="18" Alignment="Left" />
		</NewElement>
	</UpdateOrAppendChildElement>
	<UpdateOrAppendChildElement XPath=".//Chart[@Type='XY']">
		<NewElement>
			<FontLabel FontFilename="Arial" FontColor="#555555" BorderColor="#a0a0a0" FontSize="11" />
		</NewElement>
	</UpdateOrAppendChildElement>
	<UpdateOrAppendChildElement XPath=".//Chart[@Type='XY']">
		<NewElement>
			<FontData  FontFilename="Arial" FontColor="#555555" FontSize="11" />
		</NewElement>
	</UpdateOrAppendChildElement>
	<UpdateOrAppendChildElement XPath=".//Chart[@Type='XY']">
		<NewElement>
			<FontXAxisTitle FontFilename="Arial" FontColor="#555555" FontSize="11" />
		</NewElement>
	</UpdateOrAppendChildElement>
	<UpdateOrAppendChildElement XPath=".//Chart[@Type='XY']">
		<NewElement>
			<FontYAxisTitle FontFilename="Arial" FontColor="#555555" FontSize="11" />
		</NewElement>
	</UpdateOrAppendChildElement>
	<UpdateOrAppendChildElement XPath=".//Chart[@Type='XY']/SecondaryAxis">
