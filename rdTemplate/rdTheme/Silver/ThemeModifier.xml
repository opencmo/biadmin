	<CycleAttributeValues XPath=".//Chart[@Type='XY']" CycleOnChildElement="ExtraXYDataColumn">
		<CycleValue Transparency="3" Color="#ed1c24" />
		<CycleValue Transparency="3" Color="#178aff" />
		<CycleValue Transparency="3" Color="#99cc33" />
		<CycleValue Transparency="3" Color="#a36ccd" />
		<CycleValue Transparency="3" Color="#f26522" />
		<CycleValue Transparency="3" Color="#5ba6da" />
	</CycleAttributeValues>
	<CycleAttributeValues XPath=".//Chart[@Type='XY']" CycleOnChildElement="ExtraXYLayer">
		<CycleValue ChartSymbolEdgeColor="Transparent" Transparency="3" Color="#ed1c24" />
		<CycleValue ChartSymbolEdgeColor="Transparent" Transparency="3" Color="#178aff" />
		<CycleValue ChartSymbolEdgeColor="Transparent" Transparency="3" Color="#99cc33" />
		<CycleValue ChartSymbolEdgeColor="Transparent" Transparency="3" Color="#a36ccd" />
		<CycleValue ChartSymbolEdgeColor="Transparent" Transparency="3" Color="#f26522" />
		<CycleValue ChartSymbolEdgeColor="Transparent" Transparency="3" Color="#5ba6da" />
	</CycleAttributeValues>
	<CycleAttributeValues XPath=".//Chart[@Type='XY']" CycleOnChildElement="ChartMarks">
		<CycleValue Color="#ed1c24" />
		<CycleValue Color="#178aff" />
		<CycleValue Color="#99cc33" />
		<CycleValue Color="#a36ccd" />
		<CycleValue Color="#f26522" />
		<CycleValue Color="#5ba6da" />
	</CycleAttributeValues>
	<CycleAttributeValues XPath=".//Chart[@Type='XY']/SecondaryAxis" CycleOnChildElement="ChartMarks">
		<CycleValue Color="#ed1c24" />
		<CycleValue Color="#178aff" />
		<CycleValue Color="#99cc33" />
		<CycleValue Color="#a36ccd" />
		<CycleValue Color="#f26522" />
		<CycleValue Color="#5ba6da" />
	</CycleAttributeValues>

	<!-- ZoomChart -->
	<SetAttributeWhenEmpty XPath=".//ZoomChart" ResetZoomClass="rdDashboardCommand" />

	<!-- PieChart -->
<!-- No longer using a single wedge color for internal labels
	<SetAttributeWhenEmpty XPath=".//Chart[@Type='Pie' and @LabelLayout='InternalLayout']" Colors="#cccccc" />-->
	<SetAttributeWhenEmpty XPath=".//Chart[@Type='Pie']" LabelRoundedBorder="True" RoundedBorder="4" WedgeBorderThickness="1" ShowDataValues="False" BackgroundColor="Transparent" ChartSymbolEdgeColor="Transparent" OuterBorderColor="Transparent"  LabelPointerColor="SameAsWedgeColor" Colors="#cccccc,#ed1c24,#fdb913,#178aff,#99cc33,#a36ccd,#f26522,#5ba6da,#b9d989,#ad9cbe,#cb1f23,#006897,#03883b,#92278f" />
