	<SetAttributeWhenEmpty XPath=".//Chart[@XYChartType='Area' and not(ExtraXYLayer) and not(DataLayer/CrosstabFilter) ]" ChartSymbolEdgeColor="#424242" AreaGradientColor="#ffffff" />
	<SetAttributeWhenEmpty XPath=".//Chart[@XYChartType='Bar']" Transparency="3" ChartSymbolEdgeColor="Transparent" />
	<SetAttributeWhenEmpty XPath=".//Chart[@Type='XY' and @D3 and @D3 > 0]" BorderColor="#eaeaea" />
	<SetAttributeWhenEmpty XPath=".//Chart[@Type='XY']" Color="#cccccc" RoundedBorder="4" BarShape="Square" ChartBackgroundColor="Transparent" BackgroundColor="Transparent" OuterBorderColor="Transparent" BorderColor="Transparent" GridHorizontalColor="#eaeaea" LineWidth="3" />
