<TemplateModifier>
	<!-- StyleSheet -->
	<PrependChildElement XPath="/MobileReport" >
		<NewElement>
			<StyleSheet StyleSheet="rdTemplate/rdTheme/Silver/MobileTheme.css" />
		</NewElement>
    </PrependChildElement>

	<PrependChildElement XPath="/Report | /MobileReport | /Widget" >
		<NewElement>
			<StyleSheet StyleSheet="rdTemplate/rdRoundCorners.css" />
		</NewElement>
    </PrependChildElement>

    <PrependChildElement XPath="/Report | /MobileReport | /Widget" >
		<NewElement>
			<StyleSheet StyleSheet="rdTemplate/rdTheme/Silver/Theme.css" />
		</NewElement>
    </PrependChildElement>

	<!-- WaitPage -->
	<SetAttributeWhenEmpty XPath=".//WaitPage" Class="rdThemeWaitPanel" CaptionClass="rdThemeWaitCaption" StyleSheet="rdTemplate/rdTheme/Silver/Theme.css" />

	<!-- Tabs -->
	<SetAttributeWhenEmpty XPath=".//Tabs" BorderColor="#cccccc" ActiveTabBackgroundColor="#ececec" InactiveTabBackgroundColor="#898989" />
	<SetAttribute XPath="//Tabs[@ID='rdDashboardTabs']" BorderColor="#cccccc" ActiveTabBackgroundColor="#ececec" />

	<!-- DataTable -->
	<SetAttributeWhenEmpty XPath=".//DataTable" CellSpacing="1" />
	<SetAttributeWithInsert XPath=".//DataTable[not(ancestor::Report[@ID='AnalysisGrid.rdAgTemplate']) and not(@rdAnalysisGrid='True')]" Class="rdThemeDataTable" ColumnHeaderClass="rdThemeDataTableHeader" CaptionClass="rdThemeDataTableMainHeader" />
	<SetAttributeWithInsert XPath=".//DataTable/DataTableColumn | DataTableColumn" Class="rdThemeDataTableCell" />
	<SetAttributeWithInsert XPath=".//DataTable/HeaderRow" Class="rdThemeDataTableHeaderRow" />
	<SetAttributeWithInsert XPath=".//DataTable/SummaryRow" Class="rdThemeDataTableSummaryRow" />
	<CycleAttributeValues XPath=".//DataTable" CycleOnChildElement="GroupHeaderRow">
		<CycleValue Class="rdThemeDataTableGroupHeaderRow0" />
		<CycleValue Class="rdThemeDataTableGroupHeaderRow1" />
		<CycleValue Class="rdThemeDataTableGroupHeaderRow2" />
		<CycleValue Class="rdThemeDataTableGroupHeaderRow3" />
		<CycleValue Class="rdThemeDataTableGroupHeaderRow4" />
	</CycleAttributeValues>
	<SetAttributeWithInsert XPath=".//DataTable/GroupSummaryRow" Class="rdThemeDataTableGroupSummaryRow" />
	<SetAttributeWithInsert XPath=".//DataTable/MoreInfoRow" Class="rdThemeDataTableMoreInfoRow" />

	<!-- SubDataTable -->
	<SetAttributeWhenEmpty XPath=".//SubDataTable" CellSpacing="1" />
	<SetAttributeWithInsert XPath=".//SubDataTable" Class="rdThemeSubDataTable" />
	<SetAttributeWithInsert XPath=".//SubDataTable/DataTableColumn" Class="rdThemeSubDataTableDataTableCell" />
	<SetAttributeWithInsert XPath=".//SubDataTable/HeaderRow" Class="rdThemeSubDataTableHeaderRow" />
	<SetAttributeWithInsert XPath=".//SubDataTable/SummaryRow" Class="rdThemeSubDataTableSummaryRow" />
	<SetAttributeWithInsert XPath=".//SubDataTable/GroupHeaderRow" Class="rdThemeSubDataTableGroupHeaderRow" />
	<SetAttributeWithInsert XPath=".//SubDataTable/GroupSummaryRow" Class="rdThemeSubDataTableGroupSummaryRow" />
	<SetAttributeWithInsert XPath=".//SubDataTable/MoreInfoRow" Class="rdThemeSubDataTableMoreInfoRow" />

	<!-- Paging -->
	<SetAttributeWithInsert XPath=".//InteractivePaging" Class="rdThemePaging" CurrentPageClass="rdThemePagingCurrent" />
	<SetAttribute XPath=".//AnalysisGrid" CurrentPageClass="rdThemePagingCurrent" />
	<SetAttribute XPath=".//InteractivePaging[@CaptionType='Image']" FirstPageCaption="rdTemplate/rdTheme/Silver/rdPageFirst.gif" PreviousPageCaption="rdTemplate/rdTheme/Silver/rdPagePrevious.gif" NextPageCaption="rdTemplate/rdTheme/Silver/rdPageNext.gif" LastPageCaption="rdTemplate/rdTheme/Silver/rdPageLast.gif" />
	<SetAttribute XPath=".//InteractivePaging[@CaptionType='Image' and @FirstPageCaption='page_first_en.gif' ]" FirstPageCaption="rdTemplate/rdTheme/Silver/rdPageFirst.gif" />
	<SetAttribute XPath=".//InteractivePaging[@CaptionType='Image' and @PreviousPageCaption='page_prev_en.gif']" PreviousPageCaption="rdTemplate/rdTheme/Silver/rdPagePrevious.gif" />
	<SetAttribute XPath=".//InteractivePaging[@CaptionType='Image' and @NextPageCaption='page_next_en.gif']" NextPageCaption="rdTemplate/rdTheme/Silver/rdPageNext.gif" />
	<SetAttribute XPath=".//InteractivePaging[@CaptionType='Image' and @LastPageCaption='page_last_en.gif']" LastPageCaption="rdTemplate/rdTheme/Silver/rdPageLast.gif" />
	<SetAttributeWithInsert XPath=".//AppendPaging" Class="ThemeLinkButton" />

	<!-- CrossTabTable -->
	<SetAttributeWhenEmpty XPath=".//CrosstabTable" CellSpacing="1" />
	<SetAttributeWithInsert XPath=".//CrosstabTable[not(ancestor::Report[@ID='AnalysisGrid.rdAgTemplate']) and not(@rdAnalysisGrid='True')]" Class="rdThemeDataTable" ColumnHeaderClass="rdThemeDataTableHeader" />
	<SetAttributeWithInsert XPath=".//CrosstabTableLabelColumn" Class="rdThemeDataTableCell" />
	<SetAttributeWithInsert XPath=".//CrosstabTableValueColumns" Class="rdThemeDataTableCell ThemeAlignRight" />
	<SetAttributeWithInsert XPath=".//CrosstabTable/HeaderRow" Class="rdThemeDataTableHeaderRow" />
	<SetAttributeWithInsert XPath=".//CrosstabTable/SummaryRow" Class="rdThemeDataTableSummaryRow" />

	<!-- Analysis, OLAP Grid -->
	<SetAttributeWithInsert XPath=".//AnalysisGrid/AnalysisGridColumn | AnalysisGridColumn" Class="rdThemeDataTableCell" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate']//DataTable[@ID='dtLayout' or @ID='dtGroups' or @ID='dtOrders' or @ID='dtAggrs' or @ID='dtCharts' or @ID='dtCrosstabs' or @ID='dtCalcColumns' or @ID='dtFilters']" Class="rdRemoveTableStyle" />
	<SetAttribute XPath="/Report[@ID='OlapGrid.rdOgTemplate']//DataTableColumn[@ID='colPickedFilters'] | /Report[@ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='ChartGrid.rdCgTemplate']//DataTable[@ID='dtPickedFilters' or @ID='dtCalcMeasures' or @ID='mclDimensions' or @ID='mclMeasures' or @ID='mclFilters' or @ID='dtGridYAxis' or @ID='dtGridXAxis' or @ID='dtGridColumns' or @ID='dtGridRows' or @ID='dtGridFilter'] | /Report[@ID='OlapGrid.rdOgTemplate']//OlapTable[@ID='otFilterTable']" Class="rdRemoveTableStyle" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='TrellisChart.rdTcTemplate' or @ID='AnalysisChart.rdAcTemplate']//Chart" BackgroundColor="#ffffff" Transparency="" ChartTexture="" BarShading="" OuterBorderColor="#c1c1c1" GridHorizontalColor="#eaeaea" ChartBackgroundColor="#ffffff" AreaGradientColor="#ffffff" ChartBackgroundGradientColor="" LineWidth="3" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='TrellisChart.rdTcTemplate' or @ID='AnalysisChart.rdAcTemplate']//Chart/FontChartTitle" FontFilename="Arial bold" FontColor="#555555" FontSize="18" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='TrellisChart.rdTcTemplate' or @ID='AnalysisChart.rdAcTemplate']//Chart/FontLabel" FontFilename="Arial" FontColor="#555555" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='TrellisChart.rdTcTemplate' or @ID='AnalysisChart.rdAcTemplate']//Chart/HeatmapFontTitle" FontFilename="Arial" FontBold="True" FontColor="#555555" FontSize="18" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='AnalysisChart.rdAcTemplate']//Chart[@XYChartType='Area']" Transparency="7" ChartSymbolEdgeColor="Transparent" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='AnalysisChart.rdAcTemplate']//Chart[@XYChartType='Bar']" D3="" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='AnalysisChart.rdAcTemplate']//Chart[@Type='Pie']" D3Angle="" D3="" LabelShading="" WedgeBorderColor="#ffffff" LabelRoundedBorder="True" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='TrellisChart.rdTcTemplate' or @ID='AnalysisChart.rdAcTemplate']//Chart[@Type='Scatter']" BorderColor="#a0a0a0" ChartBackgroundColor="#ffffff" GridVerticalColor="#eaeaea" ChartSymbol="SymbolCircle" ChartSymbolEdgeColor="#555555" ChartSymbolFillColor="#cccccc" />
	<SetAttribute XPath="/Report[@ID='AnalysisGrid.rdAgTemplate' or @ID='OlapGrid.rdOgTemplate' or @ID='OlapGrid.rdDgTemplate' or @ID='TrellisChart.rdTcTemplate' or @ID='AnalysisChart.rdAcTemplate']//ChartTrendLine" LineWidth="3" Color="#ffbf50" />
	<SetAttribute XPath="/Report[@ID='TrellisChart.rdTcTemplate']//Chart[@Type='Scatter']" ChartSymbolSize="5" />
	<SetAttribute XPath="/Report[@ID='TrellisChart.rdTcTemplate']//Chart[@Type='XY']" LineWidth="2" D3="0" />
	<UpdateOrAppendChildElement XPath="/Report[@ID='OlapGrid.rdDgTemplate' or @ID='TrellisChart.rdTcTemplate' or @ID='OlapGrid.rdOgTemplate']//Chart[@Type='XY']">
		<NewElement>
			<DimensionGridChartPanel Colors="#cccccc,#ed1c24,#fdb913,#178aff,#99cc33,#a36ccd,#f26522,#5ba6da,#b9d989,#ad9cbe,#cb1f23,#006897,#03883b,#92278f" />
		</NewElement>
	</UpdateOrAppendChildElement>
	<UpdateOrAppendChildElement XPath="/Report[@ID='TrellisChart.rdTcTemplate']//Chart[@Type='XY' or @Type='Scatter']">
		<NewElement>
			<FontLabel FontSize="8" />
		</NewElement>
	</UpdateOrAppendChildElement>
	<UpdateOrAppendChildElement XPath="/Report[@ID='TrellisChart.rdTcTemplate']//Chart[@Type='XY' or @Type='Scatter']">
		<NewElement>
			<FontData FontSize="8" />
		</NewElement>
	</UpdateOrAppendChildElement>

	<!-- OlapTable-->
	<SetAttributeWhenEmpty XPath=".//OlapTable" CellSpacing="1" />
	<SetAttributeWithInsert XPath=".//OlapTable" Class="rdThemeDataTable" ColumnHeaderClass="rdThemeDataTableHeader" CaptionClass="rdThemeDataTableMainHeader" />
	<SetAttributeWithInsert XPath=".//OlapTable/DataTableColumn | DataTableColumn" Class="rdThemeDataTableCell" />

	<!-- PopupMenu -->
	<!--<SetAttributeWithInsert XPath=".//Action[@Type='Popup']" Class="rdThemePopupMenu" />-->

	<!-- Charts -->

	<SetAttributeWhenEmpty XPath=".//ChartTrendLine" LineWidth="3" Color="#ffbf50" />
	<SetAttributeWhenEmpty XPath=".//ChartLowessCurveLine" LineWidth="3" Color="#ffbf50" />

	<!-- Legend -->
	<SetAttributeWhenEmpty XPath=".//Legend" BorderColor="Transparent" />
	<UpdateOrAppendChildElement XPath=".//Legend">
		<NewElement>
