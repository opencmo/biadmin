		<CycleValue ChartSymbolEdgeColor="Transparent" Transparency="" Color="#d45ce2" />
		<CycleValue ChartSymbolEdgeColor="Transparent" Transparency="" Color="#e88622" />
		<CycleValue ChartSymbolEdgeColor="Transparent" Transparency="" Color="#ff4d4d" />
	</CycleAttributeValues>
	<CycleAttributeValues XPath=".//Chart[@Type='XY']" CycleOnChildElement="ChartMarks">
