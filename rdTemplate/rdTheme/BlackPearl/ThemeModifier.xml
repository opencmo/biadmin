	<!-- PieChart -->
<!-- No longer using a single wedge color for internal labels
	<SetAttributeWhenEmpty XPath=".//Chart[@Type='Pie' and @LabelLayout='InternalLayout']" Colors="#e88622" />-->
	<SetAttributeWhenEmpty XPath=".//Chart[@Type='Pie']" RoundedBorder="4" LabelRoundedBorder="True" BackgroundColor="Transparent" ChartTexture="Flat" ChartSymbolEdgeColor="Transparent" ShowDataValues="False"  OuterBorderColor="Transparent" WedgeBorderThickness="1" WedgeBorderColor="#0f0f0f" ExternalLayoutLabelColor="SameAsWedgeColor" LabelPointerColor="SameAsWedgeColor" Colors="#e88622,#28b8d5,#85d914,#d45ce2,#ff4d4d,#5a5a5a,#ffea00" />
