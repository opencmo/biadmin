	<SetAttributeWithInsert XPath=".//InteractivePaging" Class="rdThemePaging" CurrentPageClass="rdThemePagingCurrent" />
	<SetAttribute XPath=".//AnalysisGrid" CurrentPageClass="rdThemePagingCurrent" />
	<SetAttribute XPath=".//InteractivePaging[@CaptionType='Image']" FirstPageCaption="rdTemplate/rdTheme/ProfessionalBlue/rdPageFirst.gif" PreviousPageCaption="rdTemplate/rdTheme/ProfessionalBlue/rdPagePrevious.gif" NextPageCaption="rdTemplate/rdTheme/ProfessionalBlue/rdPageNext.gif" LastPageCaption="rdTemplate/rdTheme/ProfessionalBlue/rdPageLast.gif" />
