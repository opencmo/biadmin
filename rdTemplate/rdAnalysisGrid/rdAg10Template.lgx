						<InputText ID="rdAgCurrentOpenPanel" DefaultValue="@Request.rdAgCurrentOpenPanel~" />
					</Division>

					<Rows Class="rdAgPanel" CellSpacing="4px" Width="100" WidthScale="%" >
						<Row>
							<Column>
								<Rows CellSpacing="0">
									<Row>

										<Column ID="colQueryBuilder" Class="rdAgUnselectedTab">
											<EventHandler DhtmlEvent="onclick">
												<Action Type="Link">
													<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgShowMenuTab('QueryBuilder')" />
												</Action>
											</EventHandler>
											<Rows CellSpacing="0">
												<Row>
													<Column Width="1" WidthScale="%">
														<Image Caption="rdTemplate/rdAnalysisGrid/rdAgQueryBuilder.gif" ID="imgQueryBuilder" AltText="Select the data." Tooltip="Select the data." />
													</Column>
													<Column Class="rdAgTabHeading" >
														<Label Caption="Data" ID="lblHeadingQueryBuilder"/>
														<Spaces Size="1"/>
													</Column>
												</Row>
											</Rows>
										</Column>
										<Column ID="colSpacerQueryBuilder" Width="6" WidthScale="px"/>

										<Column ID="colLayout" Class="rdAgUnselectedTab">
											<EventHandler DhtmlEvent="onclick">
												<Action Type="Link">
													<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgShowMenuTab('Layout')" />
												</Action>
											</EventHandler>
											<Rows CellSpacing="0">
												<Row>
													<Column Width="1" WidthScale="%">
														<Image Caption="rdTemplate/rdAnalysisGrid/rdAgLayout.gif" ID="imgLayout" AltText="Hide and move columns." Tooltip="Hide and move columns."/>
													</Column>
													<Column Class="rdAgTabHeading" >
														<Label Caption="Columns" ID="lblHeadingLayout"/>
														<Spaces Size="1"/>
													</Column>
												</Row>
											</Rows>
										</Column>
										<Column ID="colSpacerLayout" Width="6" WidthScale="px"/>

										<Column ID="colCalc" Class="rdAgUnselectedTab">
											<EventHandler DhtmlEvent="onclick">
												<Action Type="Link">
													<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgShowMenuTab('Calc')" />
												</Action>
											</EventHandler>
											<Rows CellSpacing="0">
												<Row>
													<Column Width="1" WidthScale="%">
														<Image Caption="rdTemplate/rdAnalysisGrid/rdAgCalc.gif" ID="imgCalc" AltText="Add a new column with a formula." Tooltip="Add a new column with a formula." />
													</Column>
													<Column Class="rdAgTabHeading" >
														<Label Caption="Formula" ID="lblHeadingCalc"/>
														<Spaces Size="1"/>
													</Column>
												</Row>
											</Rows>
										</Column>
										<Column ID="colSpacerCalc" Width="6" WidthScale="px"/>

										<Column ID="colSortOrder" Class="rdAgUnselectedTab">
											<EventHandler DhtmlEvent="onclick">
												<Action Type="Link">
													<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgShowMenuTab('SortOrder')" />
												</Action>
											</EventHandler>
											<Rows CellSpacing="0">
												<Row>
													<Column Width="1" WidthScale="%">
														<Image Caption="rdTemplate/rdAnalysisGrid/rdAgSortOrder.gif" ID="imgSortOrder" AltText="Order the data on one or more columns." Tooltip="Order the data on one or more columns."/>
													</Column>
													<Column Class="rdAgTabHeading" >
														<Label Caption="Sort" ID="lblHeadingSort"/>
														<Spaces Size="1"/>
													</Column>
												</Row>
											</Rows>
										</Column>
										<Column ID="colSpacerSort" Width="6" WidthScale="px"/>

										<Column ID="colFilter" Class="rdAgUnselectedTab">
											<EventHandler DhtmlEvent="onclick">
												<Action Type="Link">
													<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgShowMenuTab('Filter')" />
												</Action>
											</EventHandler>
											<Rows CellSpacing="0">
												<Row>
													<Column Width="1" WidthScale="%">
														<Image Caption="rdTemplate/rdAnalysisGrid/rdAgFilter.gif" ID="imgFilter" AltText="Remove rows with a filter." Tooltip="Remove rows with a filter."/>
													</Column>
													<Column Class="rdAgTabHeading" >
														<Label Caption="Filter" ID="lblHeadingFilter"/>
														<Spaces Size="1"/>
													</Column>
												</Row>
											</Rows>
										</Column>
										<Column ID="colSpacerFilter" Width="6" WidthScale="px"/>

										<Column ID="colGroup" Class="rdAgUnselectedTab">
											<EventHandler DhtmlEvent="onclick">
												<Action Type="Link">
													<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgShowMenuTab('Group')" />
												</Action>
											</EventHandler>
											<Rows CellSpacing="0">
												<Row>
													<Column Width="1" WidthScale="%">
														<Image Caption="rdTemplate/rdAnalysisGrid/rdAgGroup.gif" ID="imgGroup" AltText="Group the data." Tooltip="Group the data."/>
													</Column>
													<Column Class="rdAgTabHeading" >
														<Label Caption="Group" ID="lblHeadingGroup"/>
														<Spaces Size="1"/>
													</Column>
												</Row>
											</Rows>
										</Column>
										<Column ID="colSpacerGroup" Width="6" WidthScale="px"/>

										<Column ID="colAggr" Class="rdAgUnselectedTab">
											<EventHandler DhtmlEvent="onclick">
												<Action Type="Link">
													<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgShowMenuTab('Aggr')" />
												</Action>
											</EventHandler>
											<Rows CellSpacing="0">
												<Row>
													<Column Width="1" WidthScale="%">
														<Image Caption="rdTemplate/rdAnalysisGrid/rdAgAggr.gif" ID="imgAggr" AltText="Get aggregate values like sum and count." Tooltip="Get aggregate values like sum and count."/>
													</Column>
													<Column Class="rdAgTabHeading" >
														<Label Caption="Aggregate" ID="lblHeadingAggr"/>
														<Spaces Size="1"/>
													</Column>
												</Row>
											</Rows>
										</Column>
										<Column ID="colSpacerAggr" Width="6" WidthScale="px"/>

										<Column ID="colChart" Class="rdAgUnselectedTab">
											<EventHandler DhtmlEvent="onclick">
												<Action Type="Link">
													<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgShowMenuTab('Chart')" />
												</Action>
											</EventHandler>
											<Rows CellSpacing="0">
												<Row>
													<Column Width="1" WidthScale="%" >
														<Image Caption="rdTemplate/rdAnalysisGrid/rdAgChart.gif" ID="imgChart" AltText="Add a chart." Tooltip="Add a chart."/>
													</Column>
													<Column Class="rdAgTabHeading" >
														<Label Caption="Chart" ID="lblHeadingChart"/>
														<Spaces Size="1"/>
													</Column>
												</Row>
											</Rows>
										</Column>
										<Column ID="colSpacerChart" Width="6" WidthScale="px"/>

										<Column ID="colCrosstab" Class="rdAgUnselectedTab">
											<EventHandler DhtmlEvent="onclick">
												<Action Type="Link">
													<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgShowMenuTab('Crosstab')" />
												</Action>
											</EventHandler>
											<Rows CellSpacing="0">
												<Row>
													<Column Width="1" WidthScale="%">
														<Image Caption="rdTemplate/rdAnalysisGrid/rdAgCrosstab.gif" ID="imgCrosstab" AltText="Create a crosstab view." Tooltip="Create a crosstab view."/>
													</Column>
													<Column Class="rdAgTabHeading" >
														<Label Caption="Crosstab" ID="lblHeadingCrosstab"/>
														<Spaces Size="1"/>
													</Column>
												</Row>
											</Rows>
										</Column>
										<Column ID="colSpacerCrosstab" Width="6" WidthScale="px"/>

										<Column ID="colPaging" Class="rdAgUnselectedTab">
											<EventHandler DhtmlEvent="onclick">
												<Action Type="Link">
													<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgShowMenuTab('Paging')" />
												</Action>
											</EventHandler>
											<Rows CellSpacing="0">
												<Row>
													<Column Width="1" WidthScale="%">
														<Image Caption="rdTemplate/rdAnalysisGrid/rdAgPaging.gif" ID="imgPaging" AltText="Control paging." Tooltip="Control paging."/>
													</Column>
													<Column Class="rdAgTabHeading" >
														<Label Caption="Paging" ID="lblHeadingPaging"/>
														<Spaces Size="1"/>
													</Column>
												</Row>
											</Rows>
										</Column>

									</Row>
								</Rows>

							</Column>
						</Row>
					</Rows>

				</Column>
			</Row>



			<Row ID="rowQueryBuilder" Class="rdAgContentRow" ShowModes="None">
				<Column Class="rdAgNoPadding">
					<Rows Class="rdAgPanel" Width="100" WidthScale="%">
						<Row>
							<Column ID="colQueryBuilder" >
								<Rows Class="rdAgContentHeadingRow">
									<Row>
										<Column>
											<Image Class="rdAgContentHeading" Caption="rdTemplate/rdAnalysisGrid/rdAgQueryBuilder.gif" ID="imgQueryBuilderSub" AltText="Close" Tooltip="Close">
												<Action Type="Link">
													<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgShowMenuTab('QueryBuilder')" />
												</Action>
											</Image>
											<Spaces Size="2"/>
											<Label ID="lblQueryBuilderDescription" Class="rdAgContentHeading" Caption="Select data for analysis."></Label>
										</Column>
									</Row>
								</Rows>
                
                <Division ID="divColumnError-InUse" Condition="@Session.agColumnError-InUse~">
                  <Spaces Size="3"/>
                  <Label ID="lblColumnError-InUse" Class="rdAgError" Caption="Cannot remove.  It is used in another panel.">
                  </Label>
                </Division>
                
								<Division ID="divQueryBuilder">
									<ActiveQueryBuilder ID="NorthwindQb" ConnectionID="SQL2K8tasks" Class="" TemplateModifierFile="" />
								</Division>

							</Column>
						</Row>
					</Rows >
				</Column>
			</Row>

			<Row ID="rowCalc" Class="rdAgContentRow" ShowModes="None">
				<Column Class="rdAgNoPadding">
					<Rows Class="rdAgPanel" Width="100" WidthScale="%">
						<Row>
							<Column ID="colCalc" >
								<Rows Class="rdAgContentHeadingRow">
									<Row>
										<Column>
											<Image Class="rdAgContentHeading" Caption="rdTemplate/rdAnalysisGrid/rdAgCalc.gif" ID="imgCalcSub" AltText="Close" Tooltip="Close">
												<Action Type="Link">
													<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgShowMenuTab('Calc')" />
												</Action>
											</Image>
											<Spaces Size="2"/>
											<Label ID="lblFormulaColumnDescription" Class="rdAgContentHeading" Caption="Add a new column from a formula. "></Label>
											<Label Caption="Formula Help" ID="lblCalcHelp" Class="rdAgCommand">
												<Action Type="Report" ID="actCalcHelp">
													<Target Type="Report" FrameID="NewWindow" Report="rdAg10HelpCalc" ID="tgtCalcHelp"/>
													<LinkParams rdAgStyleSheet=""/>
                          <LinkParams rdAgCommand="CalcAdd"/>
                        </Action>
											</Label>
										</Column>
									</Row>
								</Rows>

								<Division ID="divCalcAdd">
									<Rows ID="rowsColCalc">
										<Row ID="row1ColCalc">
											<Column ID="col1Row1Calc">
												<Label ID="lblCalcName" Caption="Name"  For="rdAgCalcName">
												</Label>
											</Column>
											<Column ID="col2Row1Calc">
												<InputText ID="rdAgCalcName" DefaultValue="@Request.rdAgCalcName~"/>
											</Column>
										</Row>
										<Row Class="rdAgAlignRight" ID="row2ColCalc">
											<Column ID="col1Row2Calc">
											</Column>
											<Column ID="col2Row2Calc">
												<Label ID="lblCalcInsertDataColumn" Caption="Insert a column" For="rdAgCalcDataColumns" >
												</Label>
												<Spaces Size="2"/>
												<InputSelectList OptionValueColumn="BracketedColumnName" ID="rdAgCalcDataColumns" OptionCaptionColumn="ColumnName" IncludeBlank="True">
													<DataLayer Type="Static" ID="dlCalcDataColumns">
													</DataLayer>
												</InputSelectList>
												<Spaces Size="2"/>
												<Label ID="lblCalcInsertDataColumnNow" Caption="Insert" Class="rdAgCommand">
													<Action Type="Link" ID="actCalcInsertDataColumnNow">
														<Target Type="Link" ID="tgtCalcInsertDataColumnNow" Link="javascript:document.rdForm.rdAgCalcFormula.value=document.rdForm.rdAgCalcFormula.value+document.rdForm.rdAgCalcDataColumns.value"/>
													</Action>
												</Label>
											</Column>
										</Row>
										<Row ID="row3ColCalc">
											<Column ID="col1Row3Calc">
												<Label ID="lblCalcFormula" Caption="Formula"  For="rdAgCalcFormula">
												</Label>
											</Column>
											<Column ID="col2Row3Calc">
												<InputTextArea ID="rdAgCalcFormula" InputColumns="50" InputRows="4" DefaultValue="@Request.rdAgCalcFormula~"/>
											</Column>
										</Row>
										<Row ID="row4ColCalc">
											<Column ID="col1Row4Calc">
												<Label ID="lblCalcDataType" Caption="Data Type" For="rdAgCalcDataTypes" >
												</Label>
											</Column>
											<Column ID="col2Row4Calc">
												<InputSelectList OptionValueColumn="Format" ID="rdAgCalcDataTypes" OptionCaptionColumn="Format" DefaultValue="@Request.rdAgCalcDataTypes~">
													<DataLayer Type="Static" ID="dlCalcDataTypes">
														<StaticDataRow Format="Number"/>
														<StaticDataRow Format="Text"/>
														<StaticDataRow Format="Date"/>
														<StaticDataRow Format="DateTime"/>
													</DataLayer>
												</InputSelectList>
											</Column>
										</Row>
										<Row ID="row5ColCalc">
											<Column ID="col1Row5Calc">
												<Label ID="lblCalcDisplayFormat" Caption="Display Format" For="rdAgCalcFormats">
												</Label>
											</Column>
											<Column ID="col2Row5Calc">
												<InputSelectList OptionValueColumn="Format" ID="rdAgCalcFormats" IncludeBlank="True" OptionCaptionColumn="FormatName" DefaultValue="@Request.rdAgCalcFormats~">
													<DataLayer Type="Static" ID="dlCalcFormats">
														<StaticDataRow Format="###,###,##0.00" FormatName="###,###,##0.00"/>
														<StaticDataRow Format="$###,###,##0.00" FormatName="$###,###,##0.00"/>
														<StaticDataRow Format="yyyy/MM/dd" FormatName="yyyy/MM/dd"/>
														<StaticDataRow Format="yyyy/MM/dd hh:mm:ss" FormatName="yyyy/MM/dd hh:mm:ss"/>
														<StaticDataRow Format="MM/dd/yyyy" FormatName="MM/dd/yyyy"/>
														<StaticDataRow Format="MM/dd/yyyy hh:mm:ss" FormatName="MM/dd/yyyy hh:mm:ss"/>
														<StaticDataRow Format="Bars" FormatName="Bar Gauges"/>
														<StaticDataRow Format="BackgroundColorSlider" FormatName="Cell Colors"/>
													</DataLayer>
												</InputSelectList>
											</Column>
										</Row>
										<Row ID="row7ColCalc">
											<Column />
											<Column ID="col2Row7Calc">
												<Label Caption="Add" ID="lblCalcOk" Class="rdAgCommand">
													<Action Type="Report" ID="actAddCalc">
														<Target Type="Report" ID="tgtAddCalcReport"/>
														<LinkParams rdAgCommand="CalcAdd" rdAgCommandID="@Function.GUID~"/>
													</Action>
												</Label>
												<Division ID="divCalcError-BlankName" Condition="@Session.agCalcError-BlankName~">
													<Spaces Size="3"/>
													<Label ID="lblCalcError-BlankName" Class="rdAgError" Caption="Enter a Name for the column.">
													</Label>
												</Division>
												<Division ID="divCalcError-BlankFormula" Condition="@Session.agCalcError-BlankFormula~">
													<Spaces Size="3"/>
													<Label ID="lblCalcError-BlankFormula" Class="rdAgError" Caption="Enter a Formula for the column.">
													</Label>
												</Division>
												<Division ID="divCalcError-FormulaWithNoDataColumn" Condition="@Session.agCalcError-FormulaWithNoDataColumn~">
													<Spaces Size="3"/>
													<Label ID="lblCalcError-FormulaWithNoDataColumn" Class="rdAgError" Caption="Formula must use a Data Column.">
													</Label>
												</Division>
												<Division ID="divCalcError-ActiveSql" Condition="&quot;@Request.rdAgCommand~&quot;.indexOf(&quot;Calc&quot;)==0" >
													<Division ID="divCalcError-ActiveSql" Condition="@Session.rdActiveSqlError~" >
														<Spaces Size="3"/>
														<Label ID="lblActiveSqlErrorCalc" Caption="There is an error in the formula." Class="rdAgError" />
													</Division>
												</Division>
											</Column>
										</Row>
									</Rows>
								</Division>
								<Division ID="divCalcList" ShowModes="None">
									<LineBreak LineCount="2" ID="nlCalcList"/>
									<Label Caption="Formula Columns:" ID="lblCalcColumns" Class="rdAgContentHeading"/>
									<DataTable ID="dtCalcColumns">
										<DataLayer Type="Static" ID="dlCalcColumns"/>
										<DataTableColumn ID="colCalcColumnName">
											<Label Caption="&#8226; " ID="lblBulletCalc" Format="HTML">
											</Label>
											<Label Caption="@Data.Name~" ID="lblCalcColumnName">
												<Action Type="Link" ID="actShowCalcColumn">
													<Target Type="Link" Link="javascript:
document.rdForm.rdAgCalcName.value=&quot;@Data.Name~&quot;;
document.rdForm.rdAgCalcDataTypes.value=&quot;@Data.DataType~&quot;;
document.rdForm.rdAgCalcFormula.value=&quot;@Data.Formula~&quot;.replace(/&amp;quot;/g,'&quot;').replace(/&amp;amp;/g,'&amp;').replace(/&amp;lt;/g,'&lt;');
document.rdForm.rdAgCalcFormats.value=&quot;@Data.Format~&quot;;
" ID="tgtShowCalcColumn"/>
												</Action>
											</Label>
											<Spaces Size="3"/>
										</DataTableColumn>
										<DataTableColumn ID="colCalcColumnFormula">
											<Label Caption="@Data.Formula~" ID="lblCalcColumnFormula">
											</Label>
											<Spaces Size="3"/>
										</DataTableColumn>
										<DataTableColumn ID="colCalcColumnReplaceRemove">
											<Label Caption="Replace" ID="lblCalcReplace" Class="rdAgCommand">
												<Action Type="Report" ConfirmMessage="Replace?  Are you sure?" ID="actCalcReplace">
													<Target Type="Report" ID="tgtCalcReplace"/>
													<LinkParams rdAgCommand="CalcReplace" rdAgCalcID="@Data.ID~" rdAgCommandID="@Function.GUID~"/>
												</Action>
											</Label>
											<Label Caption="Remove" ID="lblCalcRemove" Class="rdAgCommand">
												<Action Type="Report" ConfirmMessage="Remove?  Are you sure?" ID="actCalcRemove">
													<Target Type="Report" ID="tgtCalcRemove"/>
													<LinkParams rdAgCommand="CalcRemove" rdAgCalcID="@Data.ID~" rdAgCommandID="@Function.GUID~"/>
												</Action>
											</Label>
										</DataTableColumn>
									</DataTable>
									<Division ID="divCalcError-InUse" Condition="@Session.agCalcError-InUse~">
										<Spaces Size="3"/>
										<Label ID="lblCalcError-InUse" Class="rdAgError" Caption="Cannot remove.  It is used in another panel.">
										</Label>
									</Division>
									<Division ID="divCalcError-InUse2" Condition="@Session.agCalcError-InUse2~">
										<Spaces Size="3"/>
										<Label ID="lblCalcError-InUse2" Class="rdAgError" Caption="Cannot replace the Name or Data Type when used in another panel.">
										</Label>
									</Division>
								</Division>
							</Column>
						</Row>
					</Rows >
				</Column>
			</Row>

			<Row ID="rowLayout" Class="rdAgContentRow" ShowModes="None">
				<Column Class="rdAgNoPadding" >
					<Rows Class="rdAgPanel" Width="100" WidthScale="%">
						<Row>
							<Column ID="colLayout" ColSpan="2">
								<Rows Class="rdAgContentHeadingRow">
									<Row>
										<Column>
											<Image Class="rdAgContentHeading" Caption="rdTemplate/rdAnalysisGrid/rdAgLayout.gif" ID="imgLayoutSub" AltText="Close" Tooltip="Close">
												<Action Type="Link">
													<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgShowMenuTab('Layout')" />
												</Action>
											</Image>
											<Spaces Size="2"/>
											<Label  ID="lblLayoutDescription" Class="rdAgContentHeading" Caption="Hide and show columns."/>
										</Column>
									</Row>
								</Rows>

								<DataMultiColumnList ID="dtLayout" MultiListColumns="4" MultiListDirection="Down">
									<DataLayer Type="Static" ID="dlLayout" />
									<InputCheckbox ID="rdAgColVisible" DefaultValue="@Data.ColumnVisible~" CheckedValue="True" Class="rdAgColVisible"/>
									<Label Caption="@Data.Header~" ID="lblColHeader" For="rdAgColVisible_Row@Data.Row~"/>
									<InputHidden ID="rdAgLayoutColumnName" DefaultValue="@Data.ID~" />
								</DataMultiColumnList>
								<Row ID="RowLayoutControls">
									<Column ID="colLayoutControls" >
										<Label Caption="OK" ID="lblLayoutOk" Class="rdAgCommand">
											<Action Type="Report" ID="actSetLayout">
												<Target Type="Report" ID="tgtSetLayout"/>
												<LinkParams rdAgCommand="LayoutSet" rdAgCommandID="@Function.GUID~"/>
											</Action>
										</Label>
									</Column>
									<Column Class="rdAgAlignRight" ID="colLayoutControlsAllNone" >
										<Label Caption="Select: " ID="lblLayoutSelect"/>
										<Label Caption="All" ID="lblLayoutSelectAll" Class="rdAgCommand">
											<Action Type="Link">
												<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgLayoutSelect('All')" />
											</Action>
										</Label>
										<Label Caption="None" ID="lblLayoutSelectNone" Class="rdAgCommand">
											<Action Type="Link">
												<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgLayoutSelect('None')" />
											</Action>
										</Label>
									</Column>
								</Row>
							</Column>
						</Row>
					</Rows>
				</Column>
			</Row>

			<Row ID="rowSortOrder" Class="rdAgContentRow" ShowModes="None">
				<Column Class="rdAgNoPadding">
					<Rows Class="rdAgPanel" Width="100" WidthScale="%">
						<Row>
							<Column ID="colOrder">
								<Rows Class="rdAgContentHeadingRow">
									<Row>
										<Column>
											<Image Class="rdAgContentHeading" Caption="rdTemplate/rdAnalysisGrid/rdAgSortOrder.gif" ID="imgSortOrderSub" AltText="Close" Tooltip="Close">
												<Action Type="Link">
													<Target Type="Link" Link="javascript: LogiXML.AnalysisGrid.rdAgShowMenuTab('SortOrder')" />
												</Action>
											</Image>
											<Spaces Size="2"/>
											<Label  ID="lblSortOrderDescription" Class="rdAgContentHeading" Caption="Order rows by cell values."/>
